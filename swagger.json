{
  "swagger" : "2.0",
  "info" : {
    "title" : "Customer API",
    "description" : "This is a customer api created using swagger",
    "version" : "1.0.0",
    "contact" : {
      "name" : "Ranjani",
      "url" : "http://",
      "email" : "ranjani.vraman@gmail.com"
    },
    "license" : {
      "name" : "Apache '2.0'"
    }
  },
  "paths" : {
    "/customer" : {
      "get" : {
        "summary" : "Reads a customer data",
        "operationId" : "getCustomer",
        "description" : "Get customer",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "query",
          "name" : "customerId",
          "description" : "?customerId=",
          "required" : true,
          "type" : "integer"
        } ],
        "responses" : {
          "200" : {
            "description" : "response for customer",
            "schema" : {
              "type" : "object",
              "properties" : {
                "customerId" : {
                  "type" : "integer"
                },
                "accountValue" : {
                  "type" : "number"
                },
                "active" : {
                  "type" : "boolean"
                },
                "contacts" : {
                  "type" : "array",
                  "items" : {
                    "type" : "object",
                    "properties" : {
                      "name" : {
                        "type" : "string"
                      },
                      "email" : {
                        "type" : "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}